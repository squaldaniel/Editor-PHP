{
	"name": "kingkernel/editor-php",
	"description": "PHP server-side libraries for Editor",
	"type": "library",
	"require": {
		"php": ">=5.4.0 <8.4"
	},
	"keywords": ["DataTables","jQuery","Editor"],
	"homepage": "https://kingkernel.com.br",
	"authors": [
		{
			"name": "Kingkernel",
			"homepage": "https:/kingkernel.com.br"
		}
	],
	"require-dev": {
		"friendsofphp/php-cs-fixer": "~3.41.0",
		"phpstan/extension-installer": "^1.1",
		"phpstan/phpstan": "~1.10.47",
		"phpstan/phpstan-deprecation-rules": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"DataTables\\": ""
		}
	},
	"license": "MIT",
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		},
		"sort-packages": true
	}
}
